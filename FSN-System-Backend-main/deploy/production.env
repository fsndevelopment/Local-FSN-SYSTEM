# FSN Appium Farm - Production Environment Variables
# IMPORTANT: Set these values before deploying to VPS

# ===========================================
# CRITICAL LICENSING CONFIGURATION
# ===========================================
LICENSE_CLIENT_ID=YOUR_CLIENT_ID_HERE
LICENSE_KEY=YOUR_BASE64_LICENSE_KEY_HERE
LICENSE_ADMIN_SECRET_KEY=SECURE_RANDOM_STRING_FOR_LICENSE_ADMIN

# ===========================================
# DATABASE CREDENTIALS
# ===========================================
DB_PASSWORD=CHANGE_THIS_SECURE_DATABASE_PASSWORD

# ===========================================
# REDIS CREDENTIALS
# ===========================================
REDIS_PASSWORD=CHANGE_THIS_SECURE_REDIS_PASSWORD

# ===========================================
# API SECURITY
# ===========================================
API_SECRET_KEY=CHANGE_THIS_TO_VERY_SECURE_RANDOM_STRING

# ===========================================
# SSL/TLS CERTIFICATES (if using HTTPS)
# ===========================================
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# ===========================================
# DEPLOYMENT SETTINGS
# ===========================================
DEPLOYMENT_ENV=production
SERVER_LOCATION=vps
DOMAIN=your-domain.com

# ===========================================
# BACKUP CONFIGURATION
# ===========================================
BACKUP_ENABLED=true
BACKUP_S3_BUCKET=fsn-backups
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret
