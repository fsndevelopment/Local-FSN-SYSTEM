# FSN Appium Farm API Configuration - Production VPS

# App Settings
DEBUG=false
SECRET_KEY=CHANGE-THIS-TO-SECURE-RANDOM-KEY-IN-PRODUCTION
API_V1_PREFIX=/api/v1

# Database (PostgreSQL for production)
DATABASE_URL=postgresql+asyncpg://fsn_user:CHANGE_PASSWORD@localhost:5432/fsn_appium_farm

# Redis
REDIS_URL=redis://localhost:6379/0

# API Settings (Update with your VPS domain)
ALLOWED_ORIGINS=https://yourdomain.com,https://api.yourdomain.com

# Server Settings
HOST=0.0.0.0
PORT=8000

# Security
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOW_HEADERS=*
