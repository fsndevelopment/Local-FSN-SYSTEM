{
  "stories_tray": {
    "description": "Stories tray container at top of home feed",
    "evidence_path": "Appium Screenshots/USED ALREADY/",
    "strategies": [
      {
        "type": "accessibility_id",
        "value": "stories-tray",
        "confidence": 0.95,
        "notes": "Stories tray container from XML evidence"
      },
      {
        "type": "xpath",
        "value": "//XCUIElementTypeCell[@name='stories-tray']",
        "confidence": 0.9,
        "notes": "XPath for stories tray"
      }
    ],
    "verify_signals": [
      {
        "type": "element_enabled",
        "expected": true
      },
      {
        "type": "element_visible", 
        "expected": true
      }
    ],
    "timeout_ms": 5000
  },
  "story_ring": {
    "description": "Individual story ring/bubble (active, not viewed yet)",
    "evidence_path": "Appium Screenshots/USED ALREADY/",
    "strategies": [
      {
        "type": "accessibility_id",
        "value": "story-ring",
        "confidence": 0.95,
        "notes": "Validated story ring selector"
      },
      {
        "type": "ios_class_chain",
        "value": "**/XCUIElementTypeOther[`name == \"story-ring\"`][1]",
        "confidence": 0.9,
        "notes": "First story ring using class chain"
      },
      {
        "type": "xpath",
        "value": "(//XCUIElementTypeOther[@name=\"story-ring\"])[1]",
        "confidence": 0.85,
        "notes": "First story ring using XPath"
      }
    ],
    "fallback_strategies": [
      {
        "type": "xpath",
        "value": "//XCUIElementTypeOther[@name=\"story-ring\"]",
        "confidence": 0.8,
        "notes": "Any story ring (first available)"
      },
      {
        "type": "accessibility_id",
        "value": "story-tray-profile-picture",
        "confidence": 0.7,
        "notes": "Story profile picture as fallback"
      }
    ],
    "verify_signals": [
      {
        "type": "element_enabled",
        "expected": true
      },
      {
        "type": "element_visible", 
        "expected": true
      }
    ],
    "timeout_ms": 3000
  },
